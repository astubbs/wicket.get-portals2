<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
      PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
      "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

    <display-name>Wicket Examples</display-name>
    
    <!-- only available with servlet spec. 2.4 
	<locale-encoding-mapping-list>
		<locale-encoding-mapping>
			<locale>de</locale>
			<encoding>UTF-8</encoding>
		</locale-encoding-mapping>
	</locale-encoding-mapping-list>
    -->	

  <!-- Used for Springframwork2 example -->    
    <context-param>
      <param-name>contextConfigLocation</param-name> 
      <param-value>/WEB-INF/spring.xml</param-value> 
    </context-param>
    
    <!-- Prefer using the spring context listener instead of the loader servlet --> 
    <listener>
      <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class> 
    </listener>
  <!-- End-of: Used for Springframwork2 example -->    

    <servlet>
        <servlet-name>HelloWorldApplication</servlet-name>
        <servlet-class>wicket.examples.helloworld.HelloWorldApplication</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>FormInputApplication</servlet-name>
        <servlet-class>wicket.examples.forminput.FormInputApplication</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>NestedApplication</servlet-name>
        <servlet-class>wicket.examples.nested.NestedApplication</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>GuestBookApplication</servlet-name>
        <servlet-class>wicket.examples.guestbook.GuestBookApplication</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>GuestBook2Application</servlet-name>
        <servlet-class>wicket.examples.guestbook2.GuestBook2Application</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>ImagesApplication</servlet-name>
        <servlet-class>wicket.examples.images.ImagesApplication</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>LibraryApplication</servlet-name>
        <servlet-class>wicket.examples.library.LibraryApplication</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>LinkomaticApplication</servlet-name>
        <servlet-class>wicket.examples.linkomatic.LinkomaticApplication</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>NavomaticApplication</servlet-name>
        <servlet-class>wicket.examples.navomatic.NavomaticApplication</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>PubApplication</servlet-name>
        <servlet-class>wicket.examples.pub.PubApplication</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>SignInApplication</servlet-name>
        <servlet-class>wicket.examples.signin.SignInApplication</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>UploadApplication</servlet-name>
        <servlet-class>wicket.examples.upload.UploadApplication</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>SignIn2Application</servlet-name>
        <servlet-class>wicket.examples.signin2.SignIn2Application</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>DisplaytagApplication</servlet-name>
        <servlet-class>wicket.examples.displaytag.DisplaytagApplication</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>EncodingsApplication</servlet-name>
        <servlet-class>wicket.examples.encodings.EncodingsApplication</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>HelloMozillaApplication</servlet-name>
        <servlet-class>wicket.examples.hellomozilla.HelloMozillaApplication</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>SpringApplication</servlet-name>
        <!-- DO NOT USE YOUR WICKET APPLICATION. USE SPRING'S DISPATCHER SERVLET -->
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>SpringApplication2</servlet-name>
        <!-- DO NOT USE YOUR WICKET APPLICATION. USE SPRING'S DISPATCHER SERVLET -->
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>GroovyApplication</servlet-name>
        <servlet-class>wicket.examples.groovy.GroovyApplication</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <!-- servlet>
        <servlet-name>Wicket-Appfuse</servlet-name>
        <servlet-class>wicket.examples.appfuse.AppfuseApplication</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet -->

    <servlet-mapping>
        <servlet-name>SignInApplication</servlet-name>
        <url-pattern>/signin</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>NestedApplication</servlet-name>
        <url-pattern>/nested</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SignIn2Application</servlet-name>
        <url-pattern>/signin2</url-pattern>
    </servlet-mapping>
	 
    <servlet-mapping>
        <servlet-name>PubApplication</servlet-name>
        <url-pattern>/pub</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>NavomaticApplication</servlet-name>
        <url-pattern>/navomatic</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>LinkomaticApplication</servlet-name>
        <url-pattern>/linkomatic</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>LibraryApplication</servlet-name>
        <url-pattern>/library</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ImagesApplication</servlet-name>
        <url-pattern>/images</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>HelloWorldApplication</servlet-name>
        <url-pattern>/helloworld</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GuestBook2Application</servlet-name>
        <url-pattern>/guestbook2</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>FormInputApplication</servlet-name>
        <url-pattern>/forminput</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GuestBookApplication</servlet-name>
        <url-pattern>/guestbook</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>UploadApplication</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DisplaytagApplication</servlet-name>
        <url-pattern>/displaytag</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>EncodingsApplication</servlet-name>
        <url-pattern>/encodings</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>HelloMozillaApplication</servlet-name>
        <url-pattern>/hellomozilla</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SpringApplication</servlet-name>
        <url-pattern>/spring</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SpringApplication2</servlet-name>
        <url-pattern>/spring2</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GroovyApplication</servlet-name>
        <url-pattern>/groovy</url-pattern>
    </servlet-mapping>

    <!-- servlet-mapping>
        <servlet-name>Wicket-Appfuse</servlet-name>
        <url-pattern>/appfuse/*</url-pattern>
    </servlet-mapping -->

</web-app>
