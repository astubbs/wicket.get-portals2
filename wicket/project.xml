<project>
    <pomVersion>3</pomVersion>
    <id>wicket</id>
    <name>Wicket</name>
    <groupId>wicket</groupId>
    <currentVersion>0.9.14-SNAPSHOT</currentVersion>
    <organization>
        <name>Wicket developers</name>
