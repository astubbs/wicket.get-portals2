    <dependencies>
        <dependency>
            <groupId>dom4j</groupId>
            <artifactId>dom4j</artifactId>
            <version>1.4</version>
            <url>http://www.dom4j.org</url>
            <properties>
                <wicket.group>required</wicket.group>
                <wicket.description></wicket.description>
            </properties>
        </dependency>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>3.8.1</version>
            <url>http://www.junit.org</url>
            <properties>
                <wicket.group>test</wicket.group>
                <wicket.description>Used for testing Wicket</wicket.description>
            </properties>
        </dependency>
        <dependency>
            <groupId>log4j</groupId>
            <artifactId>log4j</artifactId>
            <version>1.2.8</version>
            <url>http://logging.apache.org/log4j</url>
            <properties>
                <wicket.group>optional</wicket.group>
                <wicket.description>If you use JDK style logging, this library is optional.</wicket.description>
            </properties>
        </dependency>
        <dependency>
            <groupId>ognl</groupId>
            <artifactId>ognl</artifactId>
            <version>2.6.5</version>
            <url>http://www.ognl.org</url>
            <properties>
                <wicket.group>required</wicket.group>
                <wicket.description>Used for introspection.</wicket.description>
            </properties>
        </dependency>
        <dependency>
            <groupId>servletapi</groupId>
            <artifactId>servletapi</artifactId>
            <version>2.3</version>
            <jar>servletapi-2.3.jar</jar>
            <properties>
                <wicket.group>compile</wicket.group>
                <wicket.description>Compile time only, is supplied by your application server.</wicket.description>
            </properties>
        </dependency>
        <dependency>
            <groupId>concurrent</groupId>
            <artifactId>concurrent</artifactId>
            <version>1.3.3</version>
            <properties>
                <wicket.group>required</wicket.group>
                <wicket.description></wicket.description>
            </properties>
        </dependency>
        <dependency>
            <groupId>commons-fileupload</groupId>
            <artifactId>commons-fileupload</artifactId>
            <version>1.0</version>
            <url>http://jakarta.apache.org/commons/fileupload/</url>
            <properties>
                <wicket.group>optional</wicket.group>
                <wicket.description>This library is required when you want to use the file upload component.</wicket.description>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>
        <dependency>
            <groupId>commons-logging</groupId>
            <artifactId>commons-logging</artifactId>
            <version>1.0.4</version>
            <url>http://jakarta.apache.org/commons/logging/</url>
            <properties>
                <wicket.group>required</wicket.group>
                <wicket.description>This library enables us to give you the choice of logging toolkit (JDK, Log4j, etc.)</wicket.description>
                <war.bundle>true</war.bundle>
            </properties>
        </dependency>
    </dependencies>
    <build>
        <sourceDirectory>src/java</sourceDirectory>
        <unitTestSourceDirectory>src/test</unitTestSourceDirectory>
        <unitTest>
            <includes>
                <include>**/*Test.java</include>
            </includes>
            <resources>
                <resource>
                    <directory>src/test</directory>
                    <includes>
                        <include>**</include>
                    </includes>
                    <excludes>
                        <exclude>**/*.java</exclude>
                    </excludes>
                    <filtering>false</filtering>
                </resource>
            </resources>
        </unitTest>
        <resources>
            <resource>
                <directory>src/java</directory>
                <includes>
                    <include>**</include>
                </includes>
                <excludes>
                    <exclude>**/*.java</exclude>
                </excludes>
                <filtering>false</filtering>
            </resource>
        </resources>
    </build>
    <reports>
        <report>maven-changes-plugin</report>
        <report>maven-tasklist-plugin</report>
        <report>maven-javadoc-plugin</report>
        <report>maven-junit-report-plugin</report>
        <report>maven-clover-plugin</report>
    </reports>
    <properties/>
</project>