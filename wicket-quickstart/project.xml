<?xml version="1.0" encoding="UTF-8"?>

<project>
  <pomVersion>3</pomVersion>
  <groupId>wicket-stuff</groupId>
  <id>wicket-kickstart</id>
  <name>Kick start</name>
  <currentVersion>1.0.0-rc3</currentVersion>
  <organization>
    <name>Wicket Stuff</name>
    <url>http://wicket-stuff.sourceforge.net</url>
    <logo>http://wicket-stuff.sourceforge.net/images/projectLogo.png</logo>
  </organization>
  <inceptionYear>2005</inceptionYear>
  <package>wicket</package>
  <logo>/images/projectLogo.png</logo>
  <description>A kick start project for the Wicket framework. This project will get you up and running using Jetty as a servlet container and web server. This will allow you to quickly start developing your first Wicket application.</description>
  <shortDescription>Kickstart project for web applications using Wicket.</shortDescription>
  <url>http://wicket-stuff.sourceforge.net/${pom.artifactId}</url>
  <issueTrackingUrl>http://sourceforge.net/tracker/?group_id=${maven.sourceforge.project.groupId}</issueTrackingUrl>
  <siteAddress>shell.sourceforge.net</siteAddress>
  <siteDirectory>/home/groups/w/wi/wicket-stuff/htdocs/${pom.artifactId}</siteDirectory>
  <repository>
    <connection>scm:cvs:pserver:anoncvs@cvs.sourceforge.net:/cvsroot/wicket-stuff:wicket-quickstart</connection>
    <developerConnection>scm:cvs:ext:${maven.username}@cvs.sourceforge.net:/cvsroot/wicket-stuff:wicket-quickstart</developerConnection>
    <url>http://cvs.sourceforge.net/viewcvs.py/wicket/wicket-quickstart</url>
  </repository>
  <versions>
    <version>
      <id>0.9.15-beta</id>
      <name>0.9.15-beta</name>
      <tag>WICKET_R_0_9_15_BETA</tag>
    </version>
    <version>
      <id>0.9.16-beta</id>
      <name>0.9.16-beta</name>
      <tag>WICKET_R_0_9_16_BETA</tag>
    </version>
    <version>
      <id>0.9.17-beta</id>
      <name>0.9.17-beta</name>
      <tag>WICKET_R_0_9_17_BETA</tag>
    </version>
    <version>
      <id>0.9.19-beta</id>
      <name>0.9.19-beta</name>
      <tag>WICKET_R_0_9_19_BETA</tag>
    </version>
    <version>
      <id>1.0.0-rc3</id>
      <name>1.0.0-rc3</name>
      <tag>WICKET_R_1_0_0_RC3</tag>
    </version>
  </versions>
  <dependencies>
    <dependency>
      <groupId>wicket</groupId>
      <artifactId>wicket</artifactId>
      <version>1.0.0-rc3</version>
      <type>jar</type>
      <properties>
        <eclipse.dependency>true</eclipse.dependency>
        <war.bundle>true</war.bundle>
        <wicket.group>required</wicket.group>
        <wicket.description>The Wicket jar</wicket.description>
      </properties>
    </dependency>
    <!--		
			===================================================================
			== Dependencies for using Wicket container                       ==
			===================================================================
		-->

    <dependency>
      <groupId>dom4j</groupId>
      <artifactId>dom4j</artifactId>
      <version>1.4</version>
      <url>http://www.dom4j.org</url>
      <properties>
        <wicket.group>required</wicket.group>
        <wicket.description/>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <url>http://www.junit.org</url>
      <properties>
        <wicket.group>test</wicket.group>
        <wicket.description>Used for testing Wicket</wicket.description>
        <war.bundle>false</war.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>log4j</groupId>
      <artifactId>log4j</artifactId>
      <version>1.2.8</version>
      <url>http://logging.apache.org/log4j</url>
      <properties>
        <wicket.group>optional</wicket.group>
        <wicket.description>If you use JDK style logging, this library is optional.</wicket.description>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>ognl</groupId>
      <artifactId>ognl</artifactId>
      <version>2.6.7</version>
      <url>http://www.ognl.org</url>
      <properties>
        <wicket.group>required</wicket.group>
        <wicket.description>Used for introspection.</wicket.description>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>servletapi</groupId>
      <artifactId>servletapi</artifactId>
      <version>2.3</version>
      <jar>servletapi-2.3.jar</jar>
      <properties>
        <wicket.group>compile</wicket.group>
        <wicket.description>Compile time only, is supplied by your application server.</wicket.description>
        <war.bundle>false</war.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>concurrent</groupId>
      <artifactId>concurrent</artifactId>
      <version>1.3.3</version>
      <properties>
        <wicket.group>required</wicket.group>
        <wicket.description/>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>commons-fileupload</groupId>
      <artifactId>commons-fileupload</artifactId>
      <version>1.0</version>
      <url>http://jakarta.apache.org/commons/fileupload/</url>
      <properties>
        <wicket.group>optional</wicket.group>
        <wicket.description>This library is required when you want to use the file upload component.</wicket.description>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>commons-logging</groupId>
      <artifactId>commons-logging</artifactId>
      <version>1.0.4</version>
      <url>http://jakarta.apache.org/commons/logging/</url>
      <properties>
        <wicket.group>required</wicket.group>
        <wicket.description>This library enables us to give you the choice of logging toolkit (JDK, Log4j, etc.)</wicket.description>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
    <!--		
			===================================================================
			== Dependencies for using Jetty servlet container                ==
			===================================================================
		-->

    <dependency>
      <groupId>jetty</groupId>
      <artifactId>org.mortbay.jetty</artifactId>
      <version>4.2.22</version>
      <type>jar</type>
      <url>http://www.mortbay.com</url>
      <properties>
        <war.bundle>false</war.bundle>
        <wicket.group>test</wicket.group>
        <wicket.description>Used for running the application from within your IDE.</wicket.description>
      </properties>
    </dependency>
    <dependency>
      <groupId>tomcat</groupId>
      <artifactId>jasper-runtime</artifactId>
      <version>4.1.30</version>
      <type>jar</type>
      <properties>
        <war.bundle>false</war.bundle>
        <wicket.group>test</wicket.group>
        <wicket.description>Used for running the application from within your IDE.</wicket.description>
      </properties>
    </dependency>
    <dependency>
      <groupId>tomcat</groupId>
      <artifactId>jasper-compiler</artifactId>
      <version>4.1.30</version>
      <type>jar</type>
      <properties>
        <war.bundle>false</war.bundle>
        <wicket.group>test</wicket.group>
        <wicket.description>Used for running the application from within your IDE.</wicket.description>
      </properties>
    </dependency>
  </dependencies>
  <build>
    <sourceDirectory>src/main/java</sourceDirectory>
    <unitTestSourceDirectory>src/test/java</unitTestSourceDirectory>
    <unitTest>
      <includes>
        <include>**/*Test.java</include>
      </includes>
      <resources>
        <resource>
          <directory>src/test/java</directory>
          <includes>
            <include>**</include>
          </includes>
          <excludes>
            <exclude>**/*.java</exclude>
          </excludes>
          <filtering>false</filtering>
        </resource>
      </resources>
    </unitTest>
    <resources>
      <resource>
        <directory>src/main/java</directory>
        <includes>
          <include>**</include>
        </includes>
        <excludes>
          <exclude>**/*.java</exclude>
        </excludes>
        <filtering>false</filtering>
      </resource>
      <resource>
        <directory>src/test/java</directory>
        <includes>
          <include>**</include>
        </includes>
        <excludes>
          <exclude>**/*.java</exclude>
        </excludes>
        <filtering>false</filtering>
      </resource>
    </resources>
  </build>
  <reports>
    <report>maven-license-plugin</report>
    <report>maven-changes-plugin</report>
  </reports>
  <properties/>
</project>
