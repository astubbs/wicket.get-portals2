<?xml version="1.0" encoding="UTF-8"?>

<project>
  <id>wicket-quickstart</id>
  <name>Wicket QuickStart</name>
  <currentVersion>0.9.16-beta</currentVersion>
  <package>wicket</package>
  <description><![CDATA[
	    Quickstart project for the Wicket framework. This project will get you up and running using Jetty as a]]><![CDATA[
        servlet container and web server. This will allow you to quickly start developing your first ]]><![CDATA[
            <a href="http://wicket.sourceforge.net">Wicket</a> application.]]></description>
  <shortDescription>Quickstart project for web applications using Wicket.</shortDescription>
  <url>http://wicket.sourceforge.net/QuickStart.html</url>
  <repository>
    <connection>scm:cvs:pserver:anoncvs@cvs.sourceforge.net:/cvsroot/wicket:wicket-quickstart</connection>
    <developerConnection>scm:cvs:ext:${maven.username}@cvs.sourceforge.net:/cvsroot/wicket:wicket-quickstart</developerConnection>
    <url>http://cvs.sourceforge.net/viewcvs.py/wicket/wicket-quickstart</url>
  </repository>
  <dependencies>
    <dependency>
      <groupId>wicket</groupId>
      <artifactId>wicket</artifactId>
      <version>0.9.16-beta</version>
      <type>jar</type>
      <properties>
        <war.bundle>true</war.bundle>
        <wicket.group>required</wicket.group>
        <wicket.description>The Wicket jar</wicket.description>
      </properties>
    </dependency>
    <!--		
            ===================================================================
            == Dependencies for using Wicket container                       ==
            ===================================================================
          -->

    <dependency>
      <groupId>dom4j</groupId>
      <artifactId>dom4j</artifactId>
      <version>1.4</version>
      <url>http://www.dom4j.org</url>
      <properties>
        <wicket.group>required</wicket.group>
        <wicket.description/>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <url>http://www.junit.org</url>
      <properties>
        <wicket.group>test</wicket.group>
        <wicket.description>Used for testing Wicket</wicket.description>
        <war.bundle>false</war.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>log4j</groupId>
      <artifactId>log4j</artifactId>
      <version>1.2.8</version>
      <url>http://logging.apache.org/log4j</url>
      <properties>
        <wicket.group>optional</wicket.group>
        <wicket.description>If you use JDK style logging, this library is optional.</wicket.description>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>ognl</groupId>
      <artifactId>ognl</artifactId>
      <version>2.6.5</version>
      <url>http://www.ognl.org</url>
      <properties>
        <wicket.group>required</wicket.group>
        <wicket.description>Used for introspection.</wicket.description>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>servletapi</groupId>
      <artifactId>servletapi</artifactId>
      <version>2.3</version>
      <jar>servletapi-2.3.jar</jar>
      <properties>
        <wicket.group>compile</wicket.group>
        <wicket.description>Compile time only, is supplied by your application server.</wicket.description>
        <war.bundle>false</war.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>concurrent</groupId>
      <artifactId>concurrent</artifactId>
      <version>1.3.3</version>
      <properties>
        <wicket.group>required</wicket.group>
        <wicket.description/>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>commons-fileupload</groupId>
      <artifactId>commons-fileupload</artifactId>
      <version>1.0</version>
      <url>http://jakarta.apache.org/commons/fileupload/</url>
      <properties>
        <wicket.group>optional</wicket.group>
        <wicket.description>This library is required when you want to use the file upload component.</wicket.description>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
    <dependency>
      <groupId>commons-logging</groupId>
      <artifactId>commons-logging</artifactId>
      <version>1.0.4</version>
      <url>http://jakarta.apache.org/commons/logging/</url>
      <properties>
        <wicket.group>required</wicket.group>
        <wicket.description>This library enables us to give you the choice of logging toolkit (JDK, Log4j, etc.)</wicket.description>
        <war.bundle>true</war.bundle>
      </properties>
    </dependency>
    <!--		
            ===================================================================
            == Dependencies for using Jetty servlet container                ==
            ===================================================================
          -->

    <dependency>
      <groupId>jetty</groupId>
      <artifactId>org.mortbay.jetty</artifactId>
      <version>4.2.20</version>
      <type>jar</type>
      <url>http://www.mortbay.com</url>
      <properties>
        <war.bundle>false</war.bundle>
        <wicket.group>test</wicket.group>
        <wicket.description>Used for running the application from within your IDE.</wicket.description>
      </properties>
    </dependency>
    <dependency>
      <groupId>tomcat</groupId>
      <artifactId>jasper-runtime</artifactId>
      <version>4.1.30</version>
      <type>jar</type>
      <properties>
        <war.bundle>false</war.bundle>
        <wicket.group>test</wicket.group>
        <wicket.description>Used for running the application from within your IDE.</wicket.description>
      </properties>
    </dependency>
    <dependency>
      <groupId>tomcat</groupId>
      <artifactId>jasper-compiler</artifactId>
      <version>4.1.30</version>
      <type>jar</type>
      <properties>
        <war.bundle>false</war.bundle>
        <wicket.group>test</wicket.group>
        <wicket.description>Used for running the application from within your IDE.</wicket.description>
      </properties>
    </dependency>
  </dependencies>
  <build>
    <sourceDirectory>src/main/java</sourceDirectory>
    <unitTestSourceDirectory>src/test/java</unitTestSourceDirectory>
    <unitTest>
      <includes>
        <include>**/*Test.java</include>
      </includes>
      <resources>
        <resource>
          <directory>src/test/java</directory>
          <includes>
            <include>**</include>
          </includes>
          <excludes>
            <exclude>**/*.java</exclude>
          </excludes>
          <filtering>false</filtering>
        </resource>
      </resources>
    </unitTest>
    <resources>
      <resource>
        <directory>src/main/java</directory>
        <includes>
          <include>**</include>
        </includes>
        <excludes>
          <exclude>**/*.java</exclude>
        </excludes>
        <filtering>false</filtering>
      </resource>
      <resource>
        <directory>src/test/java</directory>
        <includes>
          <include>**</include>
        </includes>
        <excludes>
          <exclude>**/*.java</exclude>
        </excludes>
        <filtering>false</filtering>
      </resource>
    </resources>
  </build>
  <reports>
    <report>maven-license-plugin</report>
  </reports>
  <properties/>
  <versions>
    <version>
      <id>0.9.15-beta</id>
      <name>0.9.15-beta</name>
      <tag>WICKET_R_0_9_15_BETA</tag>
    </version>
    <version>
      <id>0.9.16-beta</id>
      <name>0.9.16-beta</name>
      <tag>WICKET_R_0_9_16_BETA</tag>
    </version>
  </versions>
</project>
